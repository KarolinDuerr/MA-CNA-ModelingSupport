	title = {Developing a {Digital} {Entrepreneurial} {Mindset} for {Data}-{Driven}, {Cloud}-{Enabled}, and {Platform}-{Centric} {Business} {Activities}: {Practical} {Implications} and the {Impact} on {Society}},
	isbn = {978-3-030-53913-9 978-3-030-53914-6},
