	publisher = {Springer International Publishing},
	author = {Enes, Kristina},
	editor = {Schüppstuhl, Thorsten and Tracht, Kirsten and Raatz, Annika},
	year = {2022},
	doi = {10.1007/978-3-030-74032-0_21},
	pages = {253--262},
}

