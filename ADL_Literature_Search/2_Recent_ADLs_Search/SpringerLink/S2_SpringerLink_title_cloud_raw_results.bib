	number = {1},
	urldate = {2022-02-03},
	journal = {EURASIP Journal on Wireless Communications and Networking},
	author = {Feng, Longji and Xu, Shu and Zhang, Linghao and Wu, Jing and Zhang, Jidong and Chu, Chengbo and Wang, Zhenyu and Shi, Haoyang},
