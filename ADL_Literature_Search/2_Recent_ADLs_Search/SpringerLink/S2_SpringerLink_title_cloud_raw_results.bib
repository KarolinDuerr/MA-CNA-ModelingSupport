	number = {6},
	urldate = {2022-02-03},
	journal = {The VLDB Journal},
	author = {Zhang, Ji and Zhou, Ke and Li, Guoliang and Liu, Yu and Xie, Ming and Cheng, Bin and Xing, Jiashu},
